@Article{Bank2001,
author="Bank, B.
and Giusti, M.
and Heintz, J.
and Mbakop, G.M.",
title="Polar varieties and efficient real elimination",
journal="Mathematische Zeitschrift",
year="2001",
volume="238",
number="1",
pages="115--144",
abstract="Let                   {\$}S{\_}0{\$}                 be a smooth and compact real variety given by a reduced regular sequence of polynomials                   {\$}f{\_}1, {\backslash}ldots, f{\_}p{\$}                . This paper is devoted to the algorithmic problem of finding efficiently a representative point for each connected component of                   {\$}S{\_}0{\$}                 . For this purpose we exhibit explicit polynomial equations that describe the generic polar varieties of                   {\$}S{\_}0{\$}                . This leads to a procedure which solves our algorithmic problem in time that is polynomial in the (extrinsic) description length of the input equations                   {\$}f{\_}1,{\backslash}ldots, f{\_}p{\$}                 and in a suitably introduced, intrinsic geometric parameter, called the degree of the real interpretation of the given equation system                   {\$}f{\_}1,{\backslash}ldots,f{\_}p{\$}                .",
issn="1432-1823",
doi="10.1007/PL00004896",
url="http://dx.doi.org/10.1007/PL00004896"
}


